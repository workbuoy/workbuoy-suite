      }
    },
    {
      "name": "Create Pipeline",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "x-api-key",
            "value": "{{API_KEY}}"
          },
          {
            "key": "Idempotency-Key",
            "value": "test1"
          }
        ],
        "url": "{{baseUrl}}/api/v1/crm/pipelines",
        "body": {
          "mode": "raw",
          "raw": "{\"tenant_id\": \"t1\", \"name\": \"Sales\"}"
        }
      }
    }
  ]
