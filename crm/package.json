{
  "name": "workbuoy-crm",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "node -r ./lib/otel/register node_modules/.bin/next dev",
    "build": "next build",
    "start": "node -r ./lib/otel/register node_modules/.bin/next start",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "seed": "node prisma/seed.mjs",
    "test": "jest --coverage",
    "e2e": "playwright test",
    "lint": "eslint . --ext .js,.ts,.tsx || true",
    "openapi:validate": "swagger-cli validate openapi/crm.yaml"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@prisma/client": "5.17.0",
    "prom-client": "14.2.0",
    "zod": "3.23.8",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.52.1",
    "@opentelemetry/auto-instrumentations-node": "^0.52.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.1",
    "lucide-react": "^0.427.0"
  },
  "devDependencies": {
    "prisma": "5.17.0",
    "typescript": "5.9.2",
    "@types/node": "20.11.30",
    "jest": "30.1.3",
    "ts-jest": "29.1.1",
    "@types/jest": "30.0.0",
    "playwright": "1.46.0",
    "eslint": "^8.57.0"
  }
}