{
  "title": "DR Overview",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP 5xx rate (%)",
      "targets": [
        {
          "refId": "A",
          "expr": "100 * rate(wb_http_requests_errors_total[5m]) / clamp_min(rate(wb_http_requests_total[5m]), 1e-9)"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP latency p95 (ms)",
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(wb_http_request_duration_seconds_bucket[5m])) by (le)) * 1000"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Last cutover duration (ms)",
      "targets": [
        {
          "refId": "A",
          "expr": "max_over_time(wb_dr_cutover_duration_ms[24h])"
        }
      ]
    }
  ]
}
